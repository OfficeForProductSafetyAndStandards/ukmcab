@using UKMCAB.Web.UI.Models.ViewModels.Search.Shared
@model CABLegislativeAreasModel

@switch (Model.CABLegislativeAreaView)
{
    case CABLegislativeAreaView.LegislativeAreaList:
        <partial name="Partials/_LA_List" model="@Model"/>
        break;
    case CABLegislativeAreaView.PurposeOfAppointmentOnly:
        <partial name="Partials/_LA_PurposeOfAppointment" model="@Model.PurposeOfAppointments"/>
        break;
    case CABLegislativeAreaView.PurposeOfAppointmentWithCategories:
        <partial name="Partials/_LA_PurposeOfAppointmentWithCategories" model="@Model.PoaCategories"/>
        break;
    case CABLegislativeAreaView.None:
    default:
        throw new ArgumentOutOfRangeException();
}