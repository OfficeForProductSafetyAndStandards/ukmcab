}

<div class="govuk-tabs__panel cabs-panel govuk-tabs__panel--hidden  govuk-body" id="history">
    <div class="cab-detail-section">
        <partial name="Partials/_HistoryTable" model="new AuditLogHistoryTableViewModel { AuditLogHistory = Model, ReturnUrl = returnUrl }" />
    </div>
</div>