@model string[]
@{
    var modelState = ViewData.ModelState;
}
@if (!modelState.IsValid)
{
    var errors = new ErrorModelList(modelState, Model);
    <govuk-error-summary>
        @foreach (var error in errors.ErrorViewModels)
        {
            <govuk-error-summary-item href="#@error.Key">
                @error.Error
            </govuk-error-summary-item>
        }
    </govuk-error-summary>
}